<Window x:Class="DeltagerListe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeltagerListe"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="700" Background="#222">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Label Grid.Row="0"
                Content="Fornavn&#x9;&#x9;&#x9;Efternavn&#x9;&#x9;Studienummer&#x9;E-mail adresse" 
                Margin="10,1,9.6,1.2" 
                Foreground="#FFEEEEEE" 
                Padding="5,0,5,5" 
                FontWeight="Bold"/>

        <ListBox 
            HorizontalAlignment="Left" 
            Margin="10,0,0,1.2" 
            Width="674" 
            Background="{x:Null}" 
            BorderBrush="{x:Null}" 
            Foreground="#FFEEEEEE"
            Name="ListBox" Height="4" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
